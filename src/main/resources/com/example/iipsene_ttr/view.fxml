<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.VBox?>

<?import javafx.scene.control.Button?>
<VBox fx:id="list" alignment="CENTER" spacing="20.0" xmlns:fx="http://javafx.com/fxml"
      fx:controller="com.example.iipsene_ttr.Controllers.GameController">
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0"/>
    </padding>

    <Label fx:id="welcomeText"/>
    <Button fx:id="p1b" text="Player_1!" onAction="#playerSelect1"/>
    <Button fx:id="p2b" text="Player_2!" onAction="#playerSelect2"/>

    <Button text="exit" onAction="#deleteAndExit"/>
</VBox>
